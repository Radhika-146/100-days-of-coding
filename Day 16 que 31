 //Write a program to take a number as input and print its equivalent binary representation.

#include <stdio.h>
int main()
{
    int n,s=0;
    printf("enter the num");
    scanf("%d",&n);
    while (n>0)
    {
        int d=n%2;
        s=s*10+d;
        n=n/2;
    }
    printf("%d",s);
    return 0;
}
