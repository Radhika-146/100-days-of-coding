//Write a program to input two numbers and display their sum, difference, product, and quotient.


#include <stdio.h>
int main() 
{
   int num1,num2,sum,diff,pro;
   float quo;
   printf("enter num1: ");
   scanf("%d",&num1);
   printf("enter num2: ");
   scanf("%d",&num2);
   sum=num1+num2;
   printf("sum is: %d",sum);
   diff=num1-num2;
   printf("sum is: %d",diff);
   pro=num1*num2;
   printf("sum is: %d",pro);
   quo=num1/num2
   printf("quo is: %d",quo);
   
   return 0;
}
