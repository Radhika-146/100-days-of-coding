//Read and print elements of a one-dimensional array.

#include <stdio.h>
int main()
{
    int n,s=0;
    printf("enter the num");
    scanf("%d",&n);
    int arr[n];
    
    printf("enter elements:");
    for (int i=0; i<n; i++)
    {
      scanf("%d",&arr[i]);  
    }
    
    printf("sum of elements: ");
    for (int i=0; i<n; i++)
    {
      s=s+arr[i];
    }
    printf("%d",s);
    
    return 0;
}
