//Read and print elements of a one-dimensional array.

#include <stdio.h>
int main()
{
    int n;
    printf("enter the num");
    scanf("%d",&n);
    int arr[n];
    
    printf("enter elements:");
    for (int i=0; i<n; i++)
    {
      scanf("%d",&arr[i]);  
    }
    
    printf("please verify your elements");
    for (int i=0; i<n; i++)
    {
      printf("\n%d ",arr[i]);  
    }
    
    return 0;
}
